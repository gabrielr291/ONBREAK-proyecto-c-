<Window x:Class="OnBreak.Logo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnBreak"
        mc:Ignorable="d"
        Title="Logo" Height="300" Width="300">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="1"/>
                <GradientStop Color="#FF0808D1" Offset="0.628"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Ellipse Fill="White" HorizontalAlignment="Left" Height="134" Margin="64,37,0,0" Stroke="Black" VerticalAlignment="Top" Width="166" RenderTransformOrigin="0.804,0.389"/>
        <Image x:Name="image2" HorizontalAlignment="Left" Height="210" VerticalAlignment="Top" Width="209" Source="imagenes/dark_logo_transparent_background.png" Margin="39,0,0,0"/>
        <Label x:Name="Power" Content="Powered By DolphUnicorns" HorizontalAlignment="Left" Margin="111,210,0,0" VerticalAlignment="Top" Foreground="#FFFBF6F6" Background="{x:Null}"/>


    </Grid>
</Window>
